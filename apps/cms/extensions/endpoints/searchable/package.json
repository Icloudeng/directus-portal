{
	"name": "directus-extension-searchable",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-endpoint"
	],
	"directus:extension": {
		"type": "endpoint",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^9.18.1"
	},
	"scripts": {
		"extension-build": "directus-extension build && move-cli dist/index.js index.js",
		"extension-dev": "directus-extension build -w --no-minify"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.18.1",
		"@types/node": "^18.11.2",
		"typescript": "^4.8.4",
		"move-cli": "^2.0.0"
	}
}
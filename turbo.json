{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "build/**"
      ],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "src/**/*.jsx",
        "src/**/*.js",
        "test/**/*.js",
        "*.json",
        "docs/**/*.md",
        "i18n/**/*.md",
        "i18n/**/*.json",
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx",
        ".env",
        ".env.*"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
